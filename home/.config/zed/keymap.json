[
  {
    "context": "VimControl && vim_mode == normal || vim_mode == visual",
    "bindings": {
      "space e": "workspace::ToggleLeftDock",
      "space f f": "file_finder::Toggle",
      "space space": "file_finder::Toggle",
      "space c": "pane::CloseActiveItem",
      "space n": "workspace::NewFile",
      "space r": "editor::Rename",
      "space /": "editor::ToggleComments",
      "space O": "workspace::ToggleRightDock",
      "space t": "terminal_panel::Toggle",
      "f7": "terminal_panel::Toggle",
      "space l r": "editor::Rename",
      "space l f": "editor::Format",
      "space l d": "diagnostics::Deploy",
      "space l l": "editor::GoToDefinition",
      "space l a": "editor::ToggleCodeActions",
      "space l h": "editor::Hover",
      "space f w": "project_search::ToggleFocus",
      "space f c": "command_palette::Toggle",
      "space f s": "outline::Toggle",
      "space f S": "project_symbols::Toggle",
      "ctrl-h": "workspace::ActivatePaneLeft",
      "ctrl-j": "workspace::ActivatePaneDown",
      "ctrl-k": "workspace::ActivatePaneUp",
      "ctrl-l": "workspace::ActivatePaneRight",
      "ctrl-left": "workspace::DecreaseActiveDockSize",
      "ctrl-right": "workspace::IncreaseActiveDockSize",
      "ctrl-up": "workspace::DecreaseActiveDockSize",
      "ctrl-down": "workspace::IncreaseActiveDockSize",
      "ctrl-shift-h": "workspace::MovePaneLeft",
      "ctrl-shift-j": "workspace::MovePaneDown",
      "ctrl-shift-k": "workspace::MovePaneUp",
      "ctrl-shift-l": "workspace::MovePaneRight",
      "[ b": "pane::ActivatePreviousItem",
      "] b": "pane::ActivateNextItem",
      "\\": "pane::SplitDown",
      "|": "pane::SplitRight",
      "z a": "editor::ToggleFold",
      "alt-j": "editor::MoveLineDown",
      "alt-k": "editor::MoveLineUp",
      "alt-h": "editor::Outdent",
      "alt-l": "editor::Indent",
      "] m": "vim::NextMethodEnd",
      "[ m": "vim::PreviousMethodStart",
      "] ]": "vim::NextSectionStart",
      "[ [": "vim::PreviousSectionStart",
    },
  },
  {
    "context": "VimControl && vim_mode == insert",
    "bindings": {
      "j j": "vim::NormalBefore",
    },
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-q": "workspace::CloseWindow",
    },
  },
]
